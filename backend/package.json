{
  "name": "myhelper-backend",
  "version": "1.0.0",
  "description": "MyHelper Platform Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node src/migrations/runner.js migrate",
    "migrate:undo": "node src/migrations/runner.js undo",
    "migrate:status": "node src/migrations/runner.js status",
    "migration:create": "node src/migrations/runner.js create",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "care",
    "platform",
    "nodejs",
    "mysql",
    "sequelize"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "nodemailer": "^8.0.1",
    "sequelize": "^6.35.2",
    "socket.io": "^4.8.3",
    "stripe": "^20.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}
